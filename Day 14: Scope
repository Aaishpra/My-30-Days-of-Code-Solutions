
   Difference(vector <int> a){
       elements=a;
           
       }
void computeDifference(){
    int n=elements.size();
int min=100;
int max=1;

for(int i=0;i<n;++i)
{
    if(elements[i]<min){min=elements[i];}
    if(elements[i]>max){max=elements[i];}
}
maximumDifference=max-min;
}      
   
